{
  "name": "cardiac_mri_detection_ant_react18_yarn",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.19.5",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "nanoid": "^3.3.3",
    "python-shell": "^3.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-player": "^2.10.0",
    "react-scripts": "5.0.1",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.0"
  },
  "main": "src/electron-main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=src/electron-main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "dev": "concurrently -k \"cross-env yarn start\" \"yarn electron:start\""
  },
  "build": {
    "extends": null,
    "appId": "com.example.cardiac_mri_abnormalities_detection",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "src/electron-main.js",
      "src/electron-preload.js",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      "/extra/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^18.0.4"
  }
}
